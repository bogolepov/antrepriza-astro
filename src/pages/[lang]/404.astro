---
import theater from '@data/theater.json'
import dictionary from '@data/dictionary.json'

export function getStaticPaths () {
  return theater.languages.map(lng => {
    return {
      params: {lang: lng.lang_code},
      props: {pageLang: lng.lang_code, pageTitle: '' },
    };
  });
}

const { pageLang, pageTitle } = Astro.props;

import AntreprizaLayout from "@layouts/AntreprizaLayout.astro";
import NotFoundLayout from "@layouts/not_found.astro";

---

<AntreprizaLayout title={pageTitle} lang={pageLang} reservation="no" robots="noindex, nofollow">
	<NotFoundLayout lang={pageLang}/>
</AntreprizaLayout>


