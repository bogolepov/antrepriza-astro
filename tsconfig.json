{
	"extends": "astro/tsconfigs/base",
	"compilerOptions": {
		"baseUrl": ".",
		"paths": {
			"@components/*": ["src/components/*"],
			"@react-components/*": ["src/components/react/*"],
			"@layouts/*": ["src/layouts/*"],
			"@styles/*": ["src/styles/*"],
			"@data/*": ["src/data/*"],
			"@public_data/*": ["public/data/*"],
			"@svg/*": ["src/svg/*"],
			"@assets/*": ["src/assets/*"],
			"@scripts/*": ["src/scripts/*"]
		},
		"jsx": "react-jsx",
		"jsxImportSource": "react"
	}
}
